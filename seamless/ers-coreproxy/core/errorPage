<script>
function goBack() {
  window.history.back();
}

function fetchList() {
  let items = [ "invalidFields"],
      ul = document.createElement('ul');

  document.getElementById('fields').appendChild(ul);

  items.forEach(function (item) {
      let li = document.createElement('li');
      ul.appendChild(li);

      li.innerHTML += item;
  });
}
</script>
</head>
<body onload="fetchList()">
